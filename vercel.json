{
  "crons": [
    {
      "path": "/api/cron/update-board-image",
      "schedule": "0 * * * *"
    }
  ],
  "functions": {
    "src/app/api/cron/update-board-image/route.ts": {
      "maxDuration": 30
    }
  },
  "headers": [
    {
      "source": "/api/cron/(.*)",
      "headers": [
        {
          "key": "X-Robots-Tag",
          "value": "noindex, nofollow"
        }
      ]
    }
  ]
}
